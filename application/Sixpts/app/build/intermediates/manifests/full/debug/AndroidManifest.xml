<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.appspot.httpssixhundredpoints.sixpts"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="23" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/sixpts"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.appspot.httpssixhundredpoints.sixpts.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.appspot.httpssixhundredpoints.sixpts.map"
            android:label="@string/title_activity_map" >
            <intent-filter>
                <action android:name="com.appspot.httpssixhundredpoints.sixpts.map" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.appspot.httpssixhundredpoints.sixpts.Lots"
            android:label="@string/title_activity_lots" >
            <intent-filter>
                <action android:name="com.appspot.httpssixhundredpoints.sixpts.Lots" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>